<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anga.Elipsis_LMS">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ellipsis_logo1"
        android:label="@string/Ellipsis_E_Library"
        android:roundIcon="@drawable/ellipsis_logo1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.anga.Elipsis_LMS.AddUserActivity"
            android:exported="false" />
        <activity android:name="com.anga.Elipsis_LMS.EditUserInfoActivity" />
        <activity android:name="com.anga.Elipsis_LMS.CommentActivity" />
        <activity android:name="com.anga.Elipsis_LMS.EditBookActivity" />
        <activity android:name="com.anga.Elipsis_LMS.AddBookActivity" />
        <activity android:name="com.anga.Elipsis_LMS.HomeActivity" />
        <activity android:name="com.anga.Elipsis_LMS.AuthActivity" />
        <activity android:name="com.anga.Elipsis_LMS.OnBoardActivity" />
        <activity android:name="com.anga.Elipsis_LMS.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>